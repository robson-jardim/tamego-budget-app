{
    "name": "tamego-budget-app",
    "version": "1.0.4",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "pwa": "sw-precache --root=dist --config=precache-config.js",
        "build:dev": "ng build --prod --env=dev && npm run pwa",
        "build:staging": "ng build --prod --env=staging && npm run pwa",
        "build:prod": "ng build --prod --env=prod && npm run pwa",
        "loc": "cloc functions/src src/app src/shared --exclude-ext=spec.ts",
        "dev:local": "ng serve --evn=dev:local"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^5.2.9",
        "@angular/cdk": "^5.2.4",
        "@angular/common": "^5.2.9",
        "@angular/compiler": "^5.2.9",
        "@angular/core": "^5.2.9",
        "@angular/forms": "^5.2.9",
        "@angular/http": "^5.2.9",
        "@angular/material": "^5.2.4",
        "@angular/platform-browser": "^5.2.9",
        "@angular/platform-browser-dynamic": "^5.2.9",
        "@angular/router": "^5.2.9",
        "angularfire2": "^5.0.0-rc.6",
        "core-js": "^2.5.4",
        "firebase": "^4.12.1",
        "hammerjs": "^2.0.8",
        "rxjs": "^5.5.8",
        "zone.js": "^0.8.25"
    },
    "devDependencies": {
        "@angular/cli": "^1.7.4",
        "@angular/compiler-cli": "^5.2.9",
        "@angular/language-service": "^5.2.9",
        "@types/jasmine": "~2.8.6",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "~9.6.2",
        "cloc": "^2.3.3",
        "codelyzer": "~4.2.1",
        "jasmine-core": "~3.1.0",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~2.0.0",
        "karma-chrome-launcher": "~2.2.0",
        "karma-cli": "~1.0.1",
        "karma-coverage-istanbul-reporter": "^1.4.2",
        "karma-jasmine": "~1.1.1",
        "karma-jasmine-html-reporter": "^1.0.0",
        "protractor": "~5.3.1",
        "sw-precache-webpack-plugin": "^0.11.5",
        "ts-node": "~5.0.1",
        "tslint": "~5.9.1",
        "typescript": "2.6.2"
    }
}
